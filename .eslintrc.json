{
    "env": {
        "commonjs": true,
        "node": true,
        "es6":true
    },
    "extends": ["eslint:recommended","prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"off"
    },
    "globals": {
        "process":true
    }
}

